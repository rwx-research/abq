#!/usr/bin/env bash

/app/abq start \
  --bind "0.0.0.0" \
  --public-ip "213.188.214.85" \
  --port "8080" \
  --work-port "8081" \
  --negotiator-port "8082" \
  --user-token "$(cat /app/abq_user_token)" \
  --admin-token "$(cat /app/abq_admin_token)" \
  --tls-cert "$(cat /app/server.crt)" \
  --tls-key "$(cat /app/server.key)"
